Процедура СформироватьИЗагрузитьONSFAKT(Ид)
	Структура = Новый Структура;
	СТрокаТ = ЗаголовкиДокументов.НайтиПоИдентификатору(Ид);	
	
	Отбор = ЭДИ_РаботаСБазойДанных.ПолучитьПустуюЗапись();
	Отбор.Тип = ""STATE_ON_SFAKT"";
	Отбор.Дата = Формат(Число(ПреобразоватьДатуВСтроку(СтрокаТ.document_date)), ""ЧГ= "");
	Отбор.ИндексированныйАтрибут1 = СокрЛП(СтрокаТ.document_number);
	Отбор.ИндексированныйАтрибут2 = СокрЛП(СтрокаТ.file_name);
	Отбор.Владелец = ЗначениеВСтрокуВнутрСервер(СтрокаТ.Контрагент);
	Запрос = ЭДИ_РаботаСБазойДанных.ПолучитьСвойство(Отбор);	
	Если Запрос.Результат=0 тогда
		//Сообщить(Запрос.Сообщение);
		Сообщить(""Соответствующая запись не была найдена в базе данных"");
		Возврат;
	Иначе
		Свойство = Запрос.Содержимое;		
		Попытка 
			Документ = СТрокаТ.Документ;
		Исключение
			Документ = НЕОПРЕДЕЛЕНО;
		КонецПопытки;
		Корень = ЗначениеИзСтрокиВнутрСервер(Свойство.ДопАтрибуты);
	КонецЕсли;	
	Если ЗначениеЗаполнено(Документ) Тогда
		Структура.Вставить(""Документ"", ЗначениеВСтрокуВнутрСервер(Документ));
	Иначе
		Структура.Вставить(""Документ"", ПолучитьПустойСФ());
	КонецЕсли;
		
	
	Попытка
		СуммаСФБезНДС = Число(Корень.СтТовБезНДСВсего);
	Исключение
		СуммаСФБезНДС = 0;
	КонецПопытки; 
	СТруктура.Вставить(""СуммаСФБезНДС"", СуммаСФБезНДС);
	
	Попытка
		СуммаСФСНДС = Число(Корень.СтТовУчНалВсего);
	Исключение
		СуммаСФСНДС = 0;
	КонецПопытки;
	СТруктура.Вставить(""СуммаСФСНДС"",СуммаСФСНДС);

	Попытка 
		СуммаНДС = Число(Корень.СумНДС); 
	Исключение 
		Попытка
			СуммаНДС = СуммаСФСНДС - СуммаСФБезНДС;
		Исключение
			СуммаНДС = 0; 
		КонецПопытки;
	КонецПопытки; 
	Структура.Вставить(""СуммаНДС"",СуммаНДС);
	
	НомерНакладной = СтрокаМеждуПодстрокамиСтроки(,""номер_накладной:"","";"");
	НомерЗаказа = СтрокаМеждуПодстрокамиСтроки(,""номер_заказа:"","";"");
	Структура.Вставить(""НомерЗаказа"", НомерЗаказа);
	Структура.Вставить(""НомерНакладной"",НомерНакладной);
	Структура.Вставить(""ДатаДокумента"", СТрокаТ.document_date);
	Структура.Вставить(""Контрагент"", СтрокаТ.Контрагент);
	Структура.Вставить(""НомерДокумента"", Свойство.ИндексированныйАтрибут3);
	//СТруктура.Вставить(""ИмяПОстФайла"", Корень.ИмяПостФайла);
	РезультатЗагрузки = ЗагрузитьFNS_ARCHIVE(Структура);
	ДанныеПоТипуДокумента = ВернутьТипДокумета(Объект.temp, Свойство.Данные, ""xml"", 0);
	ДанныеПодписи = ДанныеПоТипуДокумента.ДанныеПОдписи;
	ИмяПостФайла = ДанныеПоТипуДокумента.ПервичныйИдФАйл;
	Если ЗначениеЗаполнено(РезультатЗагрузки) ТОгда
		РезультатЗагрузки.Вставить(""idSender"", ПолучитьFNSidОрганизации(ПолучитьОрганизациюПоДокументу(РезультатЗагрузки.Документ)));
		РезультатЗагрузки.Вставить(""idReceiver"", ПолучитьFNSidКонтрагента(ПолучитьКОнтрагентаПоДокументу(РезультатЗагрузки.Документ)));
		РезультатЗагрузки.Вставить(""Receiver"", ПолучитьGLNОрганизации(ПолучитьОрганизациюПоДокументу(РезультатЗагрузки.Документ)));
		РезультатЗагрузки.Вставить(""ИмяПостФайла"",ИмяПостФайла);
	КонецЕсли;
	ИмяСтатуса = СформироватьИзвещениеОПолучении(РезультатЗагрузки,ВернутьТипДокумета(Объект.temp, Свойство.Данные, ""xml"", 0).ДанныеПОдписи,""6"",Ложь);//Base64Строка(Новый ДвоичныеДанные(ФайлПодписи.ПолноеИмя))//,Истина
	//Подпись = Лев(ИмяСтатуса,СтрДлина(ИмяСтатуса)-3)+""p7s"";
	
	РаботаССертификатами = ЭДИ_ОбщиеИнструменты.ПолучитьМодульСертификации();
	ОпцииПодписания = РаботаССертификатами.ПолучитьНаборОпций();
  	ОпцииПодписания.СерийныйНомер = СтрЗаменить(Объект.Сертификат,"" "","""");
  	ОпцииПодписания.Пинкод = Объект.ПарольККлючуСертификата;
  	ОпцииПодписания.Режим = 0;
  	ОпцииПодписания.Метод = 1;
  	ОпцииПодписания.Тип = 1;
  	ОпцииПодписания.Кодирование = 1;
  	ОпцииПодписания.Способ = 1;
  	ОпцииПодписания.УдалитьФайлы = 1;
  	ОпцииПодписания.РегистрСерийногоНомера = 1;	  //перевести в верхний; -1 привести к нижнему; 0 - без изменений
	ОпцииПодписания.ИмяФайла = Объект.Temp+""OUT\""+ ИмяСтатуса;
    ОпцииПодписания.ИмяАрхива = Лев(ИмяСтатуса,СтрДлина(ИмяСтатуса)-3);
	
	Подписание = РаботаССертификатами.ПодписатьИЗапаковать(ОпцииПодписания);
    ЕСли Подписание.Результат = 0 тогда 
    	Сообщить(Подписание.Сообщение);            
     	Возврат;
	Иначе
		Base64Archive = Подписание.Содержимое;
    КонецЕСлИ;
КонецПроцедуры

