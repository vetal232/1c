Функция НайтиРеализацию(Номер, Дата, Контрагент) Экспорт
	ДлинаНомера = Документы.РеализацияТоваровУслуг.ПустаяСсылка().Метаданные().ДлинаНомера;//либо указывается вручную
	Номер2 = Лев(""000000000000000000000000000000"",ДлинаНомера-СтрДлина(Номер))+Номер;
	
	Запрос = Новый Запрос;
	Запрос.Текст = ""ВЫБРАТЬ РАЗРЕШЕННЫЕ
	|	РеализацияТоваровУслуг.Ссылка
	|ИЗ
	|	Документ.РеализацияТоваровУслуг КАК РеализацияТоваровУслуг
	|ГДЕ
	|	(РеализацияТоваровУслуг.Номер = &Номер
	|			ИЛИ РеализацияТоваровУслуг.Номер = &Номер2)
	|	И РеализацияТоваровУслуг.Контрагент = &Контрагент
	|	И РеализацияТоваровУслуг.Дата >= &ДатаНач
	|	И РеализацияТоваровУслуг.Дата <= &ДатаКон
	|
	|УПОРЯДОЧИТЬ ПО
	|	РеализацияТоваровУслуг.Дата УБЫВ"";
	Запрос.УстановитьПараметр(""Номер"",Номер);
	Запрос.УстановитьПараметр(""Номер2"",Номер2);
	Запрос.УстановитьПараметр(""Контрагент"",Контрагент);
	Запрос.УстановитьПараметр(""ДатаНач"",ДобавитьМесяц(Дата, -2));
	Запрос.УстановитьПараметр(""ДатаКон"",ДобавитьМесяц(Дата, 1));
	Результат = Запрос.Выполнить().Выбрать();
	Если Результат.Следующий() Тогда
		Возврат Результат.Ссылка;
	КонецЕсли; 
	
	Возврат Документы.РеализацияТоваровУслуг.ПустаяСсылка();
КонецФункции 
