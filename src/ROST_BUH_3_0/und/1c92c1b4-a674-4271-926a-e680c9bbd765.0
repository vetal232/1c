	Если БазЕд = ""PCE"" Тогда
		Единицы.Добавить(""796"");
	ИначеЕсли БазЕд = ""KGM"" Тогда
		Единицы.Добавить(""166"");
	Иначе
		Единицы.Добавить(""698"");
		Единицы.Добавить(""812"");
		Единицы.Добавить(""778"");
	КонецЕсли;
	
	ПоБазЕд = Истина;	
	
	Запрос = Новый Запрос;
	Запрос.Текст = ""ВЫБРАТЬ РАЗРЕШЕННЫЕ
	               |	ЕдиницыИзмерения.Ссылка
	               |ИЗ
	               |	Справочник.ЕдиницыИзмерения КАК ЕдиницыИзмерения
	               |ГДЕ
	               |	ЕдиницыИзмерения.Владелец = &Владелец
				    |""+?(ПоБазЕд,""	И ЕдиницыИзмерения.ЕдиницаПоКлассификатору.Код В(&Код)"","""");
	Запрос.УстановитьПараметр(""Владелец"",Номенклатура);
	Запрос.УстановитьПараметр(""Код"",Единицы);
	Результат = Запрос.Выполнить().Выбрать();
	Если Результат.Следующий() Тогда
		Возврат Результат.Ссылка
	КонецЕсли; 
КонецФункции
&НаКлиенте
Функция СформироватьСтруктуруЗаказа_НА(Сч)
	Структура = Новый Структура;
	СТрокаТ = ЗаголовкиДокументов.НайтиПоИдентификатору(Сч);	
		
	Отбор = ЭДИ_РаботаСБазойДанных.ПолучитьПустуюЗапись();
	Отбор.Тип = ""DOC_ORDER"";
	Отбор.Дата = Формат(Число(ПреобразоватьДатуВСтроку(СтрокаТ.document_date)), ""ЧГ= "");
	Отбор.ИндексированныйАтрибут1 = СокрЛП(СтрокаТ.document_number);
	Отбор.Владелец = ЗначениеВСтрокуВнутрСервер(СтрокаТ.Контрагент);
	Запрос = ЭДИ_РаботаСБазойДанных.ПолучитьСвойство(Отбор);	
	Если Запрос.Результат=0 тогда
		//Сообщить(Запрос.Сообщение);
		Сообщить(""Соответствующая запись не была найдена в базе данных"");
		Возврат Неопределено;
	Иначе
		Свойство = Запрос.Содержимое;		
		Попытка 
			Документ = СТрокаТ.Документ;
		Исключение
			Документ = НЕОПРЕДЕЛЕНО;
		КонецПопытки;
		Корень = ЗначениеИзСтрокиВнутрСервер(Свойство.ДопАтрибуты);
	КонецЕсли;
		
	ДокументЗагруженБезОшибок = Истина;
		
	EDI_ОбработкаДляСети = Ложь;	
	Если EDI_ОбработкаДляСети = Ложь Тогда
		Если ЗначениеЗаполнено(Документ.Ссылка) Тогда
			Структура.Вставить(""Документ"", ЗначениеИзСтрокиВнутрСервер(Документ.Ссылка)); 		
		Иначе
			Структура.Вставить(""Документ"", Неопределено); 		
		КонецЕсли;
		Попытка GLN_Delivery = Корень.ТочкаДоставкиГЛН Исключение GLN_Delivery = """" КонецПопытки;
		Попытка GLN_Seller = Корень.ПродавецГЛН Исключение GLN_Seller = """" КонецПопытки;
		Попытка GLN_Buyer = Корень.ПокупательГЛН Исключение GLN_Buyer = """" КонецПопытки;
			
		Попытка СТруктура.Вставить(""ДатаОтгрузки"",ПреобразоватьСтрокуВДату(Корень.ДатаДоставки)); Исключение КонецПопытки;
		Попытка Структура.Вставить(""ДатаОплаты"", СтрокаТ.document_date); Исключение КонецПопытки; 
		
		Попытка Структура.Вставить(""НомерВходящегоДокументаЭлектронногоОбмена"", СтрокаТ.document_number); Исключение КонецПопытки; 
		Попытка Структура.Вставить(""ДатаВходящегоДокументаЭлектронногоОбмена"", СтрокаТ.document_date); Исключение КонецПопытки; 
		Попытка Структура.Вставить(""НомерЗаказаКонтрагента"", СтрокаТ.document_number); Исключение КонецПопытки; 
		Попытка Структура.Вставить(""ДатаЗаказаКонтрагента"", СтрокаТ.document_date); Исключение КонецПопытки;
		
		СтруктураДанныхКлиента = ПолучитьОбъектыПоGLN(GLN_Seller,GLN_Buyer,GLN_Delivery, Истина);
		Попытка
			Структура.Вставить(""Организация"", СтруктураДанныхКлиента.Организация);
		Исключение
		КонецПопытки;
		Если ЗначениеЗаполнено(СтруктураДанныхКлиента.Партнер) Тогда
			Структура.Вставить(""Партнер"", СтруктураДанныхКлиента.Партнер);
			Структура.Вставить(""Грузополучатель"", СтруктураДанныхКлиента.Партнер);			
		Иначе
			Сообщить(""Не удалось найти точку доставки с GLN ""+GLN_Delivery+ "" документ не может быть загружен."",СтатусСообщения.Важное);
			Возврат Неопределено;
		КонецЕсли; 
		
		Если ЗначениеЗаполнено(СтруктураДанныхКлиента.Контрагент) Тогда //и (EDI_Контрагент=1) 
			Структура.Вставить(""Контрагент"", СтруктураДанныхКлиента.Контрагент);
		КонецЕсли;			
		//
		Если ЗначениеЗаполнено(СтруктураДанныхКлиента.ДоговорКонтрагента) Тогда
			Структура.Вставить(""Договор"", СтруктураДанныхКлиента.ДоговорКонтрагента);
		КонецЕсли;
				
		Если ЗначениеЗаполнено(СтруктураДанныхКлиента.алкПунктРазгрузки) Тогда
			Структура.Вставить(""алкПунктРазгрузки"", СтруктураДанныхКлиента.алкПунктРазгрузки);
			Структура.Вставить(""алкПунктРазгрузкиОрганизации"", Документ.алкПунктРазгрузки);
		КонецЕсли;			
		
		Попытка
			ЦенаВключаетНДС = Документ.ЦенаВключаетНДС;
		Исключение
			Попытка
				ЦенаВключаетНДС = Документ.СуммаВключаетНДС;
			Исключение
				ЦенаВключаетНДС = Неопределено;
			КонецПопытки; 
		КонецПопытки; 				
		
		Попытка
			Если Документ.ЭтоНовый() Тогда
				Структура.Вставить(""Дата"", СтрокаТ.document_date);
			КонецЕсли;		
		Исключение
		КонецПопытки;
		КоличествоИтераций = Число(Корень.КоличествоСТрокТЧ);	
		СтруктураСтроки = Новый СТруктура;		
		СтруктураСтроки.Вставить(""Количество"");
		СтруктураСтроки.Вставить(""Номенклатура"");
		СтруктураСтроки.Вставить(""КоличествоУпаковок"");
		СтруктураСтроки.Вставить(""Цена"");
		сч = 0;
		Для каждого СТрокаТЧ Из Корень.ТабличнаяЧасть ЦИкл
			сч = сч+1;
			Попытка Штрихкод = СтрокаТЧ.Значение.Штрихкод ;Исключение Штрихкод = """";КонецПопытки; 
			Попытка КодБаер = СтрокаТЧ.Значение.КодПокупателя;Исключение КодБаер = """";КонецПопытки;
			Попытка КодСапл = СтрокаТЧ.Значение.КодПродавца;Исключение КодСапл = """";КонецПопытки; 
			Попытка Коэффициент = Число(СтрокаТЧ.Значение.Коэффициент) Исключение Коэффициент = 1 КонецПопытки; 
			Попытка БазоваяЕдиница = СтрокаТЧ.Значение.ЕдиницаИзмерения Исключение БазоваяЕдиница = """" КонецПопытки; 
			//	
			Попытка
				ЦенаВФайлеБезНДС= Число(СтрокаТЧ.Значение.ЦенаБезНДС);
			Исключение
				Попытка
					ЦенаВФайлеБезНДС = Число(СтрокаТЧ.Значение.ЦенаСНдс)/(1+Число(СтрокаТЧ.Значение.СтавкаНДС)/100);
				Исключение
					ЦенаВФайлеБезНДС = 0;
				КонецПопытки; 
			КонецПопытки;
				
			ПОпытка
				ЦенаВФайлеСНДС = Число(СтрокаТЧ.Значение.ЦенаСНдс);
			ИСключение
				Попытка
					ЦенаВФайлеСНДС = Число(СтрокаТЧ.Значение.ЦенаБезНДС)*(1+Число(СтрокаТЧ.Значение.СтавкаНДС)/100);
				Исключение
					ЦенаВФайлеСНДС = 0;
				КонецПопытки; 
			КонецПопытки;
				
			СтруктураСтроки.Количество = Число(СтрокаТЧ.Значение.ЗаказанноеКоличество);	
			
						
			
			СтруктураСтроки.Номенклатура = ПолучитьТовар(КодБаер,КодСапл,Штрихкод);
			
			Если НЕ ЗначениеЗаполнено(СтруктураСтроки.Номенклатура) Тогда
				ДокументЗагруженБезОшибок = Ложь;				
				Продолжить;
			КонецЕсли;
						
			//СтруктураСтроки.КоличествоУпаковок = Количество;			
			Если Объект.EDI_Номенклатура_ЦенаИзФайла Тогда
				Если ЦенаВключаетНДС = Истина Тогда
					Цена = ЦенаВФайлеСНДС;
				ИначеЕсли ЦенаВключаетНДС = Ложь Тогда
					Цена = ЦенаВФайлеБезНДС;
				Иначе
					Цена = 0;
				КонецЕсли; 
				СтруктураСтроки.Цена = Цена;
			КонецЕсли;			
			КлючСтроки = ""Строка""+Строка(сч);
			Структура.Вставить(КлючСтроки, СТруктураСтроки);			
		КонецЦикла;
	КонецЕсли; 
	ЗагрузитьORDER(Структура);
КонецФункции
&НаКлиенте
Процедура Загрузить(Команда)
	
	Если ЭтаФорма.Элементы.Входящие.ТекущаяСтраница.Имя = ""ORDER"" тогда
		
		Для Каждого СТрокаТ Из ЗаголовкиДокументов ЦИкл
			
			Если СтрокаТ.Флажок Тогда
				
				Сч = СтрокаТ.ПолучитьИдентификатор();
				
				Попытка
					СформироватьСтруктуруЗаказа(Сч);
				Исключение
					Сообщить(ОписаниеОшибки());
				КонецПопытки;
				
			КонецЕсли;
			
		КонецЦикла;
		
		Если Объект.РежимОтладки Тогда
			
			ЗаголовкиДокументов.Очистить();
			ОбновитьИзБазыКлиент();
			
		Иначе		
			ОбновитьТаблицу(ЭтаФорма.Элементы.Входящие.ТекущаяСтраница);
		КонецЕсли;
		
	ИначеЕсли ЭтаФорма.Элементы.Входящие.ТекущаяСтраница.Имя = ""DESADV"" тогда
		Для Каждого СТрокаТ Из ЗаголовкиДокументов ЦИкл
			Если СтрокаТ.Флажок Тогда
				Сч = СтрокаТ.ПолучитьИдентификатор();
				ЗагрузитьDESADV(Сч);
			КонецЕсли;
		КонецЦикла;
		ОбновитьТаблицу(ЭтаФорма.Элементы.Входящие.ТекущаяСтраница);
		Если Объект.РежимОтладки Тогда
			ПолучитьФорму(""Документ.ПоступлениеТоваровУслуг.Форма.ФормаДокумента"",,СтрокаТ.Документ).Открыть();
		КонецЕсли;
	ИначеЕсли ЭтаФорма.Элементы.Входящие.ТекущаяСтраница.Имя = ""INVOICE"" тогда
		Для Каждого СТрокаТ Из ЗаголовкиДокументов ЦИкл
			Если СтрокаТ.Флажок Тогда
				/////
				Сч = СтрокаТ.ПолучитьИдентификатор();
				//ЗагрузитьINVOICE(Сч);
			КонецЕсли;
		КонецЦикла;
		ОбновитьТаблицу(ЭтаФорма.Элементы.Входящие.ТекущаяСтраница);
		
		Если Объект.РежимОтладки Тогда
			ПолучитьФорму(""Документ.СчетФактураПолученный.Форма.ФормаДокумента"",,СтрокаТ.Документ).Открыть();
		КонецЕсли;
		
	ИначеЕсли ЭтаФорма.Элементы.Входящие.ТекущаяСтраница.Имя = ""RECADV"" тогда
		
		Для Каждого СТрокаТ Из ЗаголовкиДокументов ЦИкл
			
			Если СтрокаТ.Флажок Тогда
				
				Сч = СтрокаТ.ПолучитьИдентификатор();
				РезультатЗагрузки = ОбработатьУведомлениеОПолучении(Сч);
				
			КонецЕсли;
			
		КонецЦикла;
		
		Если Объект.РежимОтладки Тогда
			
			ЗаголовкиДокументов.Очистить();
			ОбновитьИзБазыКлиент();
			
		Иначе		
			ОбновитьТаблицу(ЭтаФорма.Элементы.Входящие.ТекущаяСтраница);
		КонецЕсли;

	ИначеЕсли ЭтаФорма.Элементы.Входящие.ТекущаяСтраница.Имя = ""ORDRSP"" тогда
		
		Для Каждого СТрокаТ Из ЗаголовкиДокументов ЦИкл
			
			Если СтрокаТ.Флажок Тогда
				
				Сч = СтрокаТ.ПолучитьИдентификатор();
				СформироватьСТруктураОтветаНаЗаказ(Сч);
			
			КонецЕсли;
			
		КонецЦикла;
		
		Если Объект.РежимОтладки Тогда
			
			ЗаголовкиДокументов.Очистить();
			ОбновитьИзБазыКлиент();
			
		Иначе		
			ОбновитьТаблицу(ЭтаФорма.Элементы.Входящие.ТекущаяСтраница);
		КонецЕсли;
		
	ИначеЕсли ЭтаФорма.Элементы.Входящие.ТекущаяСтраница.Имя = ""FNS_ARCHIVE"" тогда
		
		Для Каждого СТрокаТ Из ЗаголовкиДокументов ЦИкл
			
			Если СтрокаТ.Флажок Тогда
				
				Сч = СтрокаТ.ПолучитьИдентификатор();
				СформироватьИЗагрузитьONSFAKT(Сч);
				
			КонецЕсли;
			
		КонецЦикла;
		
		Если Объект.РежимОтладки Тогда				
			ОбновитьИзБазыКлиент();			
		Иначе		
			ОбновитьТаблицу(ЭтаФорма.Элементы.Входящие.ТекущаяСтраница);
		КонецЕсли;
		
	КонецЕсли;	
	
КонецПроцедуры
&НаКлиенте
Функция ЗагрузитьПоSOAP(СтрокаТ, дополнить = ЛОЖЬ) Экспорт
	
	ДвоичныеДанные = ReceiveBinary(СтрокаТ.partner_iln, СтрокаТ.document_type, СтрокаТ.tracking_id);
	
	ПапкаПоДате = Формат(СтрокаТ.document_date,""ДФ=ггггММдд"")+Объект.Слэш;
	СоздатьКаталог(Объект.temp+СтрокаТ.Document_type+Объект.Слэш+ПапкаПоДате);
	
	ДвоичныеДанные.Записать(Объект.temp+СтрокаТ.Document_type+""\""+ПапкаПоДате+СтрокаТ.file_name);
	Попытка
			ЧтениеХМЛ = Новый ЧтениеXML;
			ЧтениеХМЛ.ОткрытьФайл(Объект.temp+СтрокаТ.Document_type+""\""+ПапкаПоДате+СтрокаТ.file_name);
			ЧтениеХМЛ.Прочитать();
			ЧтениеХМЛ.Закрыть();
	Исключение
		Попытка ЧтениеХМЛ.Закрыть(); Исключение КонецПопытки; 
		УдалитьФайлы(Объект.temp+СтрокаТ.Document_type+""\""+ПапкаПоДате+СтрокаТ.file_name);
		Сообщить(""Не удалось скачать файл. Повторите попытку позднее."");
		Возврат Неопределено;
	КонецПопытки;

	Если Не Объект.РежимОтладки Тогда
		Попытка
			ChangeDocumentStatus(СтрокаТ.tracking_id);
			СтрокаТ.Статус = 1;
			СтрокаТ.Флажок = Ложь;
		Исключение
			Сообщить(ОписаниеОшибки(),СтатусСообщения.Важное);
		КонецПопытки;
	КонецЕсли;
	Если дополнить Тогда
		ЧтениеXML = Новый ЧтениеXML;
		Фабрика = Новый ФабрикаXDTO;
		ЧтениеXML.ОткрытьФайл(Объект.temp+СтрокаТ.Document_type+""\""+ПапкаПоДате+СтрокаТ.file_name);
		Попытка
			Корень = Фабрика.ПрочитатьXML(ЧтениеXML);
		Исключение
			Сообщить(""Файл ""+Объект.temp+СтрокаТ.Document_type+""\""+ПапкаПоДате+СтрокаТ.file_name+"" не является файлом XML"");
			Возврат неопределено;
		КонецПопытки;				
		Контрагент = ПолучитьКонтрагента(Корень.Order_Parties.Buyer.ILN);//Sender1//Buyer1
		ДатаДоставки = ПреобразоватьСтрокуВДату(Корень.Order_Header.ExpectedDeliveryDate);
		СтрокаТ.СтруктураФайла.Добавить(Корень,""Корень"");
		СтрокаТ.Контрагент = КОнтрагент;
		СтрокаТ.ДатаДоставки = ДатаДоставки;
	КонецЕсли;
	
КонецФункции

