Функция ПолучитьСОглашение(Партнер, Организация)
	Запрос = Новый Запрос;
	Запрос.Текст = ""ВЫБРАТЬ
	               |	СоглашенияСКлиентами.Ссылка
	               |ИЗ
	               |	Справочник.СоглашенияСКлиентами КАК СоглашенияСКлиентами
	               |ГДЕ
	               |	СоглашенияСКлиентами.Организация.Ссылка = &Организация
	               |	И СоглашенияСКлиентами.Партнер.Ссылка = &Партнер"";
	Запрос.УстановитьПараметр(""Партнер"", Партнер);
	Запрос.УстановитьПараметр(""Организация"", Организация);
	РЕзультат = Запрос.Выполнить().Выбрать();
	Если Результат.Следующий() Тогда
		ВОзврат результат.Ссылка;
	Иначе
		Запрос.Текст = ""ВЫБРАТЬ
		               |	СоглашенияСКлиентами.Ссылка
		               |ИЗ
		               |	Справочник.СоглашенияСКлиентами КАК СоглашенияСКлиентами
		               |ГДЕ
		               |	СоглашенияСКлиентами.Организация.Ссылка = &Организация"";
		Запрос.УстановитьПараметр(""Организация"", Организация);
		РЕзультат = Запрос.Выполнить().Выбрать();
		Если Результат.Следующий() Тогда
			Сообщить (""Не удалось получить соглашение для организации "" + Организация + "" и партнера "" + Партнер+"". Используется типовое соглашение"");
			ВОзврат результат.Ссылка;
		Иначе
			Сообщить(""Не удалось получить соглашение для организации "" + Организация);
			Возврат Справочники.СоглашенияСКлиентами.ПустаяСсылка();
		КонецЕсли;
	КонецЕсли;
