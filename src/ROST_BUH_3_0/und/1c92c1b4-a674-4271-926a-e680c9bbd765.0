		СтруктураПоискаАдресаДоставки.Вставить(""Вид"", Справочники.ВидыКонтактнойИнформации.НайтиПоНаименованию(""Фактический адрес контрагента""));			
	ИначеЕсли Параметр = 1 Тогда
		СтруктураПоискаАдресаДоставки.Вставить(""Вид"", Справочники.ВидыКонтактнойИнформации.НайтиПоНаименованию(""Адрес""));			
	КонецЕсли;
	Возврат СтруктураПоискаАдресаДоставки;
КонецФункции

Функция ПолучитьДоговорИСоглашение(КОнтрагент, Организация, Партнер=Неопределено)
	СТруктураВозврата = Новый Структура;
	Если ЗначениеЗаполнено(КОнтрагент) И ЗначениеЗаполнено(Организация) Тогда
		Запрос = Новый Запрос;
			Запрос.Текст = ""ВЫБРАТЬ
			               |	ДоговорыКонтрагентов.Ссылка
			               |ИЗ
			               |	Справочник.ДоговорыКонтрагентов КАК ДоговорыКонтрагентов
			               |ГДЕ
			               |	ДоговорыКонтрагентов.Организация = &Организация
			               |	И ДоговорыКонтрагентов.Владелец = &Владелец
			               |	И ДоговорыКонтрагентов.ПометкаУдаления = ЛОЖЬ"";
			//Запрос.УстановитьПараметр(""Контрагент"", КОнтрагент);
			Запрос.УстановитьПараметр(""Организация"", Организация);
			Запрос.УстановитьПараметр(""Владелец"", КОнтрагент);

		РезультатЗапроса = Запрос.Выполнить().Выбрать();
		Если РезультатЗапроса.Следующий() Тогда
			СтруктураВозврата.Вставить(""ДоговорКонтрагента"",РезультатЗапроса.Ссылка);
		Иначе
			СтруктураВозврата.Вставить(""ДоговорКонтрагента"", Неопределено);
		КонецЕсли;
		Если Объект.Бухгалтерия = Ложь Тогда		
			Запрос.Текст = ""ВЫБРАТЬ
			               |	СоглашенияСКлиентами.Ссылка
			               |ИЗ
			               |	Справочник.СоглашенияСКлиентами КАК СоглашенияСКлиентами
			               |ГДЕ
			               |	СоглашенияСКлиентами.Партнер.Ссылка = &Партнер
			               |	И СоглашенияСКлиентами.Организация.Ссылка = &Организация"";
			//Запрос.УстановитьПараметр(""Контрагент"", КОнтрагент);
			Запрос.УстановитьПараметр(""Организация"", Организация);
			Запрос.УстановитьПараметр(""Партнер"", Партнер);
			РезультатЗапроса = Запрос.Выполнить().Выбрать();
			Если РезультатЗапроса.Следующий() Тогда
				СтруктураВозврата.Вставить(""Соглашение"",РезультатЗапроса.Ссылка);
			Иначе            
				Запрос.Текст = ""ВЫБРАТЬ
				               |	СоглашенияСКлиентами.Ссылка
				               |ИЗ
				               |	Справочник.СоглашенияСКлиентами КАК СоглашенияСКлиентами
				               |ГДЕ
				               |	СоглашенияСКлиентами.Организация.Ссылка = &Организация"";
				//Запрос.УстановитьПараметр(""Контрагент"", КОнтрагент);
				Запрос.УстановитьПараметр(""Организация"", Организация);
				//Запрос.УстановитьПараметр(""Партнер"", Партнер);
				РезультатЗапроса = Запрос.Выполнить().Выбрать();
				Если РезультатЗапроса.Следующий() Тогда
					СтруктураВозврата.Вставить(""Соглашение"",РезультатЗапроса.Ссылка);
				Иначе
					СтруктураВозврата.Вставить(""Соглашение"",Неопределено);
				КонецЕсли;
			КонецЕсли;
		Иначе
			СтруктураВозврата.Вставить(""Соглашение"",Неопределено);
		КонецЕсли;
	КонецЕсли;
	Возврат СТруктураВозврата;
КонецФункции

