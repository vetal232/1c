Функция ВернутьТипДокумета(ИмяФайла, ДанныеФайла = """", Расширение=""xml"", ПОлучитьИнвойс=0)
	СтруктураВозврата = Новый СТруктура;
	СтруктураВозврата.Вставить(""ТипДокумента"");
