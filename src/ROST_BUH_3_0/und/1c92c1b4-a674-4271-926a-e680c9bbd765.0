Функция ПреобразоватьДатуВСтроку(ДатаДляПреобразования)
	Результат = Формат(Год(ДатаДляПреобразования),""ЧГ=0"") + ?(СтрДлина(Строка(Месяц(ДатаДляПреобразования))) < 2,""0"" + Строка(Месяц(ДатаДляПреобразования)) , Строка(Месяц(ДатаДляПреобразования)))
                + ?(СтрДлина(Строка(День(ДатаДляПреобразования))) < 2, ""0"" + Строка(День(ДатаДляПреобразования)), Строка(День(ДатаДляПреобразования)));
    Возврат Результат;	
КонецФункции

&НаКлиенте
Функция ЗаписатьСвойство(Тип, ОбъектСвойства = Неопределено, Владелец, Атр1="""",Атр2="""",Атр3="""",Атр4="""",Атр5="""", Данные, ДопАтр="""", ДатаСвойства, состояние = 0)
	
	Если ЗначениеЗаполнено(ОбъектСвойства) Тогда
		СтрОбъект = ОбъектСвойства;
	Иначе
		СтрОбъект = """";
	КонецЕсли;
	Если ЗначениеЗаполнено(Владелец) Тогда 
		СтрВладелец = ЗначениеВСтрокуВнутрСервер(Владелец);
	Иначе
		СтрВладелец = """";
	КонецЕсли;	
	СтрДатаСвойства = ДатаСвойства;
	//ОбъектОбработка = РеквизитФормыВЗначение(""Объект"");
	НоваяЗапись = ЭДИ_РаботаСБазойДанных.ПолучитьПустуюЗапись(); 
	НоваяЗапись.Объект = СтрОбъект;
	НоваяЗапись.Владелец = СтрВладелец;
	НоваяЗапись.Тип = Тип;
	НоваяЗапись.Дата = Формат(Число(ДатаСвойства),""ЧГ="");
	НоваяЗапись.Данные = Данные;
	НоваяЗапись.ДопАтрибуты = ДопАтр;
	НоваяЗапись.Состояние = состояние;
	НоваяЗапись.ИндексированныйАтрибут1 = Атр1;
	НоваяЗапись.ИндексированныйАтрибут2 = Атр2;
	НоваяЗапись.ИндексированныйАтрибут3 = Атр3;
	НоваяЗапись.ИндексированныйАтрибут4 = Атр4;
	НоваяЗапись.ИндексированныйАтрибут5 = Атр5;
	Результат = ЭДИ_РаботаСБазойДанных.ЗаписатьСвойство(НоваяЗапись);
	Если Результат.Результат = 0 Тогда 
		Возврат Результат.Сообщение;
	Иначе 
		Возврат ""0""
	КонецЕсли;
	
КонецФункции

&НаКлиенте
Функция ПолучитьСписокДокументов(Тип, ДатаНачала, ДатаОкончания, КОнтрагентФильтр)
	
	GLNОрганизации = ПолучитьGLNОрганизации(Объект.Организация);
	
	
	Отбор = ЭДИ_РаботаСБазойДанных.ПолучитьПустуюЗапись();
	Если ЗначениеЗаполнено(КОнтрагентФильтр) Тогда
		Отбор.Владелец = ПРеобразоватьОбъектВСтроку(КОнтрагентФильтр);		
	КонецЕсли;
	Отбор.Тип = Тип;
	Если ЗначениеЗаполнено(GLNОрганизации) Тогда
		Отбор.ИндексированныйАтрибут4 = СокрЛП(GLNОрганизации);
	КонецЕсли;
	Запрос = ЭДИ_РаботаСБазойДанных.ПолучитьСписокСвойств(Отбор, ПреобразоватьДатуВСтроку(ДатаНачала), ПреобразоватьДатуВСтроку(ДатаОкончания));	
	Если Запрос.Результат=0 тогда
		Сообщить(""Нет доступных для загрузки документов."");//нет записей по заданному отбору
	Иначе
		СписокДанных = Запрос.Содержимое;
		КоличествоИтераций = СписокДанных.Верхнийиндекс();
		Если КоличествоИтераций = 0 Тогда
			ШагСостояния = 100;
		Иначе			
			ШагСостояния = 100/КоличествоИтераций;
		КонецЕсли;
		СостояниеВыполнения = 0;
		Для Сч=0 По КоличествоИтераций Цикл
			Свойство = СписокДанных.Получить(Сч);
			НоваяСтрока = ЗаголовкиДокументов.Добавить();
			НоваяСтрока.Document_type = СтрЗаменить(Тип,""DOC_"","""");
			НоваяСтрока.ИндексЗаписи = Свойство.Индекс; 
			
			НоваяСтрока.Контрагент = ПолучитьСсылкуНаОбъектИзСтроки(Свойство.Владелец);	   
			
			Если Тип = ""DOC_RECADV"" Тогда
				ДатаСтрока = СтрЗаменить(Строка(Свойство.ИндексированныйАтрибут2),"","","""");
				ДатаСтрока = СтрЗаменить(Строка(ДатаСтрока),"" "","""");
				ДатаСтрока = Лев(ДатаСтрока,4) +""-""+ Сред(ДатаСтрока,5,2) +""-""+  Прав(ДатаСтрока,2);
				ДатаДокумента = ПреобразоватьСтрокуВДату(ДатаСтрока);
				НоваяСТрока.document_date = ДатаДокумента; //для документов здесь храним дату входящего			
				НоваяСТрока.document_number = Свойство.ИндексированныйАтрибут5; //для документов здесь храним номер входящего документа
			Иначе
				ДатаСтрока = СтрЗаменить(Строка(Свойство.Дата),"","","""");
				ДатаСтрока = СтрЗаменить(Строка(ДатаСтрока),"" "","""");
				ДатаСтрока = Лев(ДатаСтрока,4) +""-""+ Сред(ДатаСтрока,5,2) +""-""+  Прав(ДатаСтрока,2);
				ДатаДокумента = ПреобразоватьСтрокуВДату(ДатаСтрока);
				НоваяСТрока.document_date = ДатаДокумента; //для документов здесь храним дату входящего			
				НоваяСТрока.document_number = Свойство.ИндексированныйАтрибут1; //для документов здесь храним номер входящего документа
			КонецЕсли;
			НоваяСтрока.СтруктураФайла = Свойство.ДопАтрибуты;
			СтруктураФайла = ЗначениеИзСтрокиВнутрСервер(Свойство.ДопАтрибуты);
			
			Если СтруктураФайла.Свойство(""ДатаДоставки"") ТОгда
				НоваяСТрока.ДатаДоставки = ПреобразоватьСтрокуВДату(СтруктураФайла.ДатаДоставки); 
			ИНаче
				НоваяСТрока.ДатаДоставки = ДатаДокумента + 60*60*24;
			КонецЕсли;
			
			Если Не ЗначениеЗаполнено(НоваяСтрока.Контрагент) Тогда
				
				GLN_Контрагента = СокрЛП(СтруктураФайла.ПокупательГЛН);
				КонтрагентСсылка = ПолучитьКонтрагента(GLN_Контрагента);
				НоваяСтрока.КОнтрагент = КонтрагентСсылка;//ПолучитьКОнтрагентаПоПартнеру(ПартнерСсылка);
				Если ЗначениеЗаполнено(НоваяСтрока.Контрагент) Тогда
					Свойство.Владелец = ЗначениеВСтрокуВнутрСервер(НоваяСтрока.Контрагент);
					РезультатЗагруки = ЭДИ_РаботаСБазойДанных.ПерезаписатьПоИндексу(Свойство);
					Если РезультатЗагруки.Результат() <> 1 Тогда
						Сообщить(РезультатЗагруки.Сообщение);
					КонецЕсли;
				КонецЕсли;
			КонецЕсли;
			Попытка			
				НоваяСтрока.Документ = ПолучитьСсылкуНаОбъектИзСтроки(Свойство.Объект);
			Исключение
				Попытка
					НоваяСТрока.Документ = НайтиЗаказ(НоваяСТрока.document_number,НоваяСТрока.document_date, Ложь);				
				Исключение
				КонецПопытки;
			КонецПопытки;;
			Попытка
				НоваяСТрока.Статус = Свойство.Состояние;
			Исключение
				Сообщить(ОписаниеОшибки());
			КонецПопытки;
