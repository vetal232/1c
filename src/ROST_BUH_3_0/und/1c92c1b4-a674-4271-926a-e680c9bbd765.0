			КонецЕсли;
			
			Если ЧтениеXML.ТипУзла = ТипУзлаXML.Текст Тогда
				
			КонецЕсли;
			
		КонецЦикла;
	Исключение
		//Сообщить(""Ответ EDI Service: ""+СтрокаXML);
