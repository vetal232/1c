	Возврат Новый Структура(
		""БезВозвратнойТары,ОтмененоБезВозвратнойТары,Отменено"",
		""Сумма,СуммаНДС,СуммаСНДС,СуммаАвтоматическойСкидки,СуммаРучнойСкидки"",
		""Сумма,СуммаНДС,СуммаСНДС,СуммаАвтоматическойСкидки,СуммаРучнойСкидки"",
		""Сумма,СуммаНДС,СуммаСНДС,СуммаАвтоматическойСкидки,СуммаРучнойСкидки"");
	
	КонецФункции
	
Функция ЗагрузитьORDRSP(СТруктураОтветаНаЗаказ)
	
	Документ = СТруктураОтветаНаЗаказ.Документ;
	
	//Корень = ПолучитьСТруктуруФайла(СТрокаТ.СтруктураФайла);
	ДокументЗагруженБезОшибок = Истина;
	Объект.ТаблицаСравненияФайлаИДокумента.Очистить();
		
	Если Не ЗначениеЗаполнено(Документ.Ссылка) Тогда
		Сообщить(""При загрузке документа  были обнаружены ошибки: не найден исходный заказ"");//с номером ""+СтрокаТ.document_number+"" от ""+СтрокаТ.document_date+"" (""+СтрокаТ.file_name+"")
		Возврат Неопределено;
	КонецЕсли;
	
	Если Ложь Тогда
		Документ = Документы.ЗаказПоставщику.СоздатьДокумент();
	КонецЕсли; 
	
	Объект.ИсправляемыйДокументRECADV = Документ.Ссылка;	
	Для каждого СтрокаТоваров Из Документ.Товары Цикл
		СтрокаСравнения = ОБъект.ТаблицаСравненияФайлаИДокумента.Добавить();
		СтрокаСравнения.Номенклатура = СтрокаТоваров.Номенклатура;
		СтрокаСравнения.КоличествоВДокументе = СтрокаТоваров.Количество;
		СтрокаСравнения.КоличествоВФайле = 0;
		СтрокаСравнения.ЦенаВДокументе = СтрокаТоваров.Цена;
		СтрокаСравнения.ЦенаВФайле = 0;
		СтрокаСравнения.ЕстьВДокументе = Истина;
		СтрокаСравнения.ЕстьВФайле = Ложь;
		//СтрокаСравнения.Флажок = Истина;
	КонецЦикла;
	
	Попытка
		ЦенаВключаетНДС = Документ.ЦенаВключаетНДС;
	Исключение
		Попытка
			ЦенаВключаетНДС = Документ.Договор.ТипЦен.ЦенаВключаетНДС;
		Исключение
			ЦенаВключаетНДС = Неопределено;
		КонецПопытки; 
	КонецПопытки;
		
	КоличествоИтераций = СТруктураОтветаНаЗаказ.ТабличнаяЧасть.Количество;
	Для Счетчик=1 по КоличествоИтераций Цикл
		Попытка
			Ключ = ""Строка"" + Строка(Счетчик);
			СтруктураСтроки = СТруктураОтветаНаЗаказ.ТабличнаяЧасть[Ключ];
			//Тег = ?(КоличествоИтераций = 1,Корень.OrderResponse_Lines.Line,Корень.OrderResponse_Lines.Line[Счетчик-1]);
			//
			//Попытка Штрихкод = Тег.Line_Item.EAN; Исключение Штрихкод = """"; КонецПопытки; 
			//Попытка КодБаер = Тег.Line_Item.BuyerItemCode; Исключение КодБаер = """"; КонецПопытки;
			//Попытка КодСапл = Тег.Line_Item.SupplierItemCode; Исключение КодСапл = """"; КонецПопытки;
			//Попытка Количество = Число(Тег.Line_Item.AllocatedDelivered); Исключение Количество = 0; КонецПопытки;
			//
			Если ЦенаВключаетНДС = Ложь Тогда
				Попытка
					Цена = СтруктураСТроки.ЦенаБезНДС;
				Исключение					
				КонецПопытки; 
			Иначе
				Попытка
					Цена = СтруктураСТроки.ЦенаСНДС;
				Исключение				
				КонецПопытки; 
			КонецЕсли;
		Исключение
			Сообщить(""В файле не найдена товарная строка №""+Счетчик);
			Продолжить;
		КонецПопытки;
		
		Количество = СтруктураСтроки.Количество;
		Номенклатура = СтруктураСтроки.Номенклатура;		
		Если НЕ ЗначениеЗаполнено(Номенклатура) Тогда
			ДокументЗагруженБезОшибок = Ложь;
			СтрокаСравнения = Объект.ТаблицаСравненияФайлаИДокумента.Добавить();
			//СтрокаСравнения.Номенклатура = ""Не найденная номенклатура: ""+?(ЗначениеЗаполнено(Штрихкод),"" с штрихкодом ""+Штрихкод+"";"","""")
			//+?(ЗначениеЗаполнено(КодБаер),"" с кодом покупателя ""+КодБаер+"";"","""")
			//+?(ЗначениеЗаполнено(КодСапл),"" с кодом поставщика ""+КодСапл+"";"","""")
			//+?(ЗначениеЗаполнено(СтрокаТ.Контрагент),"" для контрагента ""+СтрокаТ.Контрагент+"";"","""");
			
			СтрокаСравнения.КоличествоВФайле = Количество;
			СтрокаСравнения.ЦенаВФайле = Цена;
			СтрокаСравнения.ЕстьВДокументе = Ложь;
			СтрокаСравнения.ЕстьВФайле = Истина;
			//СтрокаСравнения.Флажок = Истина;
		Иначе
			СтрокаСравнения = Объект.ТаблицаСравненияФайлаИДокумента.НайтиСтроки(Новый СТруктура(""Номенклатура"",Номенклатура));
			Попытка				
				СтрокаСравнения[0].КоличествоВФайле = Количество;
				СтрокаСравнения[0].ЦенаВФайле = Цена;
				СтрокаСравнения[0].ЕстьВДокументе = Истина;
				СтрокаСравнения[0].ЕстьВФайле = Истина;
				Если СтрокаСравнения[0].КоличествоВФайле <> СтрокаСравнения[0].КоличествоВДокументе Тогда
					ДокументЗагруженБезОшибок = Ложь;
				КонецЕсли;
				Если (Объект.RECADVИсправляетЦены = Истина) и (СтрокаСравнения[0].ЦенаВФайле <> СтрокаСравнения[0].ЦенаВДокументе) Тогда
					ДокументЗагруженБезОшибок = Ложь;
				КонецЕсли;
				СтрокаСравнения[0].Флажок = Истина;
			Исключение
				СтрокаСравнения = Объект.ТаблицаСравненияФайлаИДокумента.Добавить();
				СтрокаСравнения.Номенклатура = Номенклатура;
				СтрокаСравнения.КоличествоВФайле = Количество;
				СтрокаСравнения.ЦенаВФайле = Цена;
				СтрокаСравнения.ЕстьВДокументе = Ложь;
				СтрокаСравнения.ЕстьВФайле = Истина;
				СтрокаСравнения.Флажок = Истина;
				ДокументЗагруженБезОшибок = Ложь;
			КонецПопытки;
		КонецЕсли;
	КОнецЦикла;
	
	Если Объект.ТаблицаСравненияФайлаИДокумента.Количество() <> Документ.Товары.Количество() Тогда
		ДокументЗагруженБезОшибок = Ложь;
	КонецЕсли; 
	
	//
	Если ДокументЗагруженБезОшибок = Ложь Тогда
		Если Объект.ТаблицаСравненияФайлаИДокумента.Итог(""КоличествоВФайле"") = 0 Тогда
			Сообщить(""Документ ""+Документ.Ссылка+"" отклонен продавцом!"", СтатусСообщения.Внимание);						
		Иначе
			Сообщить(""Обнаружены расхождения в документе ""+Документ.Ссылка+"" и ответе на заказ!"", СтатусСообщения.Внимание);			
		КонецЕсли; 
		Возврат Неопределено;
	Иначе
		//Для каждого СтрокаСравнения Из ФормаСравнения.ТаблицаСравненияЦеныИКоличества Цикл
		//	ТабличнаяСтрока = Документ.Товары.Найти(СтрокаСравнения.Номенклатура,""Номенклатура"");
		//	Если ЗначениеЗаполнено(ТабличнаяСтрока) Тогда
		//		ТабличнаяСтрока.КоличествоФактическое = СтрокаСравнения.КоличествоВФайле;
		//		Попытка ТабличнаяСтрока.ЦенаФактическая = СтрокаСравнения.ЦенаВФайле; Исключение КонецПопытки;
		//		//Иначе
