Функция ПолучитьСтруктуруXML(Файл,НаОсновеФайла = Истина)
	ЧтениеXML = Новый ЧтениеXML;
	Если НаОсновеФайла Тогда
		ЧтениеXML.ОткрытьФайл(Файл);
	Иначе
		ЧтениеXML.УстановитьСтроку(Файл);
	КонецЕсли; 
	
	Родитель = Неопределено;
	Корень = Новый Массив;
	Корень.Добавить(Новый Структура);//Потомки (доступ по имени)
	Корень.Добавить(Новый Структура);//Атрибуты (доступ по имени)
	Корень.Добавить("""");//Значение (строка)
	Стек = Новый Массив;
	Стек.Добавить(""Корень"");
	Пока ЧтениеXML.Прочитать() Цикл
		Если ЧтениеXML.ТипУзла = ТипУзлаXML.НачалоЭлемента и ЧтениеXML.ИмеетИмя Тогда
			Массив = Новый Массив;
			Массив.Добавить(Новый Структура);
			Массив.Добавить(Новый Структура);
			Массив.Добавить("""");
			ИмяПотомка = СтрЗаменить(ЧтениеXML.ЛокальноеИмя,""-"",""_"");
			ИмяПотомка = СтрЗаменить(ИмяПотомка,""."",""_"");
			
			Если ЧтениеXML.КоличествоАтрибутов()>0 Тогда
				Для Инд = 0 По ЧтениеXML.КоличествоАтрибутов()-1 Цикл
					Атрибут = ЧтениеXML.ЗначениеАтрибута(Инд);
					ИмяАтрибута = ЧтениеXML.ЛокальноеИмяАтрибута(Инд);
					Массив[1].Вставить(ИмяАтрибута, Атрибут);
				КонецЦикла; 
			КонецЕсли; 
			
			Если Родитель = Неопределено Тогда
				СтекКопия = Новый Массив;
				Для каждого Эл Из Стек Цикл
					СтекКопия.Добавить(Эл);
				КонецЦикла;
				КореньКопия = Новый Массив;
				КореньКопия.Добавить(Корень[0]);
				Родитель = ПолучитьТекущегоРодителя(КореньКопия,СтекКопия);
			КонецЕсли; 
			
			Инд = 1;
			Пока Истина Цикл
				Если Родитель[0].Свойство(ИмяПотомка+Строка(Инд)) Тогда
					Инд = Инд+1;
				Иначе
					Прервать;
				КонецЕсли; 
			КонецЦикла; 
			ИмяПотомка = ИмяПотомка+Строка(Инд);
			Родитель[0].Вставить(ИмяПотомка,Массив);
			Стек.Добавить(ИмяПотомка);
			Родитель[0].Свойство(ИмяПотомка,Родитель);
		КонецЕсли;
		
		Если ЧтениеXML.ИмеетЗначение Тогда
				Родитель[2] = СокрЛП(ЧтениеXML.Значение);
		КонецЕсли;
		
		Если ЧтениеXML.ТипУзла = ТипУзлаXML.КонецЭлемента Тогда
			Стек.Удалить(Стек.ВГраница());
			Родитель = Неопределено;
		КонецЕсли; 
	КонецЦикла;
	ЧтениеXML.Закрыть();
	
	Возврат Корень;
КонецФункции

Функция ПолучитьТекущегоРодителя(Корень, Стек)
	Если Стек.Количество() = 1 Тогда
		Возврат Корень;
	КонецЕсли; 
	Если Корень[0].Свойство(Стек[1],Корень) Тогда
		Стек.Удалить(1);
		Возврат ПолучитьТекущегоРодителя(Корень, Стек);
	КонецЕсли; 
КонецФункции
