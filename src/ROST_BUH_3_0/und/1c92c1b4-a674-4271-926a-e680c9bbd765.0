Функция ОбработатьУведомлениеОПолучении(Ид)
	СтруктураRECADV = Новый Структура;
	СтрокаТ = ЗаголовкиДокументов.НайтиПоИдентификатору(ИД);
		
	Отбор = ЭДИ_РаботаСБазойДанных.ПолучитьПустуюЗапись();
	Отбор.Тип = ""DOC_RECADV"";
	Отбор.Дата = Число(ПреобразоватьДатуВСтроку(СтрокаТ.document_date));
	Отбор.ИндексированныйАтрибут5 = СокрЛП(СтрокаТ.document_number);
	Отбор.Владелец = ЗначениеВСтрокуВнутрСервер(СтрокаТ.Контрагент);
	Отбор.Объект = ЗначениеВСтрокуВнутрСервер(СтрокаТ.Документ);
	Запрос = ЭДИ_РаботаСБазойДанных.ПолучитьСвойство(Отбор);	
	Если Запрос.Результат=0 тогда
		Сообщить(Запрос.Сообщение);
		Возврат Неопределено;//Неопределено;
	Иначе
		Свойство = Запрос.Содержимое;		
		Попытка 
			Документ = СТрокаТ.Документ;
		Исключение
			Документ = Неопределено;
		КонецПопытки;
		Корень = ЗначениеИзСтрокиВнутрСервер(Свойство.ДопАтрибуты);
	КонецЕсли;
	СтруктураRECADV.Вставить(""Документ"", Документ);
	Если Не ЗначениеЗаполнено(Документ) Тогда
		Сообщить(""При загрузке документа с номером ""+СтрокаТ.document_number+"" от ""+СтрокаТ.document_date+"" (""+СтрокаТ.file_name+"") были обнаружены ошибки: не найдена исходная реализация"");
		Возврат Неопределено;//Неопределено;
	КонецЕсли;
	
	СтруктураТабличнаяЧасать = Новый Структура;
	сч = 0;
	////////////////обрабатываем товарные линии файла
	КоличествоИтераций = Число(Корень.КоличествоСТрокТЧ);		
	Для каждого СТрокаТЧ Из Корень.ТабличнаяЧасть ЦИкл
		СТруктураСтроки = Новый Структура;
		СТруктураСтроки.Вставить(""ЦенаВФайлеБезНДС"");
		СТруктураСтроки.Вставить(""ЦенаВФайлеСНДС"");
		СТруктураСтроки.Вставить(""ЦенаВФайле"");
		СТруктураСтроки.Вставить(""ЦенаВДокументе"");
		СТруктураСтроки.Вставить(""КоличествоВФайле"");
		СТруктураСтроки.Вставить(""Номенклатура"");			
		СТруктураСтроки.Вставить(""КоличествоВДокументе"");		
		СТруктураСтроки.Вставить(""ЕстьВФайле"");	
		СТруктураСтроки.Вставить(""ЕстьВДокументе"");	
		Попытка Штрихкод = СтрокаТЧ.Значение.Штрихкод ;Исключение Штрихкод = """";КонецПопытки; 
		Попытка КодБаер = СтрокаТЧ.Значение.КодПокупателя;Исключение КодБаер = """";КонецПопытки;
		Попытка КодСапл = СтрокаТЧ.Значение.КодПродавца;Исключение КодСапл = """";КонецПопытки; 
		Попытка Коэффициент = Число(СтрокаТЧ.Значение.Коэффициент) Исключение Коэффициент = 1 КонецПопытки; 
		Попытка БазоваяЕдиница = СтрокаТЧ.Значение.ЕдиницаИзмерения Исключение БазоваяЕдиница = """" КонецПопытки; 
		Попытка
			ЦенаВФайлеБезНДС = Число(СтрокаТЧ.Значение.ЦенаБезНДС);
		Исключение
			Попытка
				ЦенаВФайлеБезНДС = Число(СтрокаТЧ.Значение.ЦенаСНдс)/(1+Число(СтрокаТЧ.Значение.СтавкаНДС)/100);
			Исключение
				ЦенаВФайлеБезНДС = 0;
			КонецПопытки; 
		КонецПопытки;
		СТруктураСтроки.ЦенаВФайлеБезНДС =ЦенаВФайлеБезНДС; 	
		ПОпытка
			ЦенаВФайлеСНДС = Число(СтрокаТЧ.Значение.ЦенаСНдс);
		ИСключение
			Попытка
				ЦенаВФайлеСНДС = Число(СтрокаТЧ.Значение.ЦенаБезНДС)*(1+Число(СтрокаТЧ.Значение.СтавкаНДС)/100);
			Исключение
				ЦенаВФайлеСНДС = 0;
			КонецПопытки; 
		КонецПопытки;
		СТруктураСтроки.ЦенаВФайлеСНДС = ЦенаВФайлеСНДС;
		СТруктураСтроки.КоличествоВФайле = Число(СтрокаТЧ.Значение.Количество);		
		ТоварИХарактеристика = ПолучитьТовар(КодБаер,КодСапл,Штрихкод);	
		СТруктураСтроки.Номенклатура = ТоварИХарактеристика.Номенклатура;
		СТруктураСтроки.ЕстьВФайле = Истина;
		Ключ = ""Строка"" + Строка(Сч);
		СтруктураТабличнаяЧасать.Вставить(Ключ, СТруктураСтроки);
		Сч = Сч + 1;
	КОнецЦикла;
	СтруктураТабличнаяЧасать = ЗаполнитьСТруктуруТабличнойЧастиRECADV(СтруктураТабличнаяЧасать, Документ);
	СтруктураRECADV.Вставить(""ТабличнаяЧасть"", СтруктураТабличнаяЧасать);
	Объект.ТаблицаСравненияФайлаИДокумента.Очистить();
	Для сч = 0 По СтруктураТабличнаяЧасать.Количество()-1 Цикл
		Ключ = ""Строка"" + Строка(сч);
		СтруктураСтрока = СтруктураТабличнаяЧасать[ключ];
		СтрокаСравнения = Объект.ТаблицаСравненияФайлаИДокумента.Добавить();
		СтрокаСравнения.Номенклатура = СтруктураСтрока.Номенклатура;
		СтрокаСравнения.КоличествоВДокументе = СтруктураСтрока.КоличествоВДокументе;
		СтрокаСравнения.КоличествоВФайле = СтруктураСтрока.КоличествоВФайле;
		СтрокаСравнения.ЦенаВДокументе = СтруктураСтрока.ЦенаВДокументе;
		СтрокаСравнения.ЦенаВФайле =СтруктураСтрока.ЦенаВФайле;
		СтрокаСравнения.ЕстьВДокументе = СтруктураСтрока.ЕстьВДокументе;
		СтрокаСравнения.ЕстьВФайле = СтруктураСтрока.ЕстьВФайле;
	КонецЦикла;
	
	Объект.ИсправляемыйДокументRECADV = Документ;
	
	ЗаписатьНастройкиДляСравнения();
	СтруктНастроек = Новый Структура;
	СтруктНастроек.Вставить(""Бухгалтерия"",Объект.Бухгалтерия);	
	ФормаСравнения = ПолучитьФорму(""ВнешняяОбработка.EDISOFT.Форма.ФормаСравненияФайлаИДокумента"",СтруктНастроек);	
	ФормаСравнения.ОткрытьМодально();				
	Свойство.Состояние = 1;
	ЭДИ_РаботаСБазойДанных.ПерезаписатьПоИндексу(Свойство);	
КонецФункции

Функция ЗаполнитьСТруктуруТабличнойЧастиRECADV(СтруктураТабличнаяЧасать, Документ)
	Попытка
		ЦенаВключаетНДС = Документ.ЦенаВключаетНДС;
	Исключение
		Попытка
			ЦенаВключаетНДС = Документ.ДоговорКонтрагента.ТипЦен.ЦенаВключаетНДС;
		Исключение
			ЦенаВключаетНДС = Неопределено;
		КонецПопытки; 
	КонецПопытки;
	
	Для сч = 0 По СтруктураТабличнаяЧасать.Количество()-1 Цикл
		ключ = ""Строка"" + Строка(сч);
		СтруктураСтрока = СтруктураТабличнаяЧасать[ключ];
		СтрокаТаблицы = Документ.Товары.Найти(СтруктураСтрока.Номенклатура);
		Если ЗначениеЗаполнено(СтрокаТаблицы) ТОгда 			
			ЦенаВФайле = ?(ЦенаВключаетНДС, СтруктураСтрока.ЦенаВФайлеСНДС, СтруктураСтрока.ЦенаВФайлеБезНДС);
			СтруктураСтрока.ЦенаВФайле = ЦенаВФайле;
			СтруктураСтрока.ЦенаВДокументе = СтрокаТаблицы.Цена;
			СТруктураСтрока.КоличествоВДокументе = СтрокаТаблицы.Количество;
			СтруктураСтрока.ЕстьВДокументе = Истина;
		Иначе
			СтруктураСтрока.ЕстьВДокументе = Ложь;
		КонецЕсли;
	КонецЦикла;
	Возврат СтруктураТабличнаяЧасать;
КонецФункции
