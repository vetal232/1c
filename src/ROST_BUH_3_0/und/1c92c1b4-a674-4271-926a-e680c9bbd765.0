				СтрКОнтрагент = Свойство.Объект;
				Партн = ПроверитьТипПартнёраНаСервере(СтрКОнтрагент,ИскомыйТип);
				Если ЗначениеЗаполнено(Партн) Тогда
					Возврат Партн;
				КонецЕсли;
				
			Иначе
				СтрКОнтрагент = Свойство.Объект;
				Возврат ЗначениеИзСтрокиВнутрСервер(СтрКОнтрагент);
			КонецЕсли;
			
		КонецЦикла;
	КонецЕсли;
	///////////////////////////////////////
	
	ПартнерПОле = СокрЛП(Объект.Партнер_Поле);		
	Партнер = ПолучитьПартнераСервер(ПартнерПОле,GLN, Объект.МестоПоискаПартнера);	
	Если Сообщать Тогда
		Если Не ЗначениеЗаполнено(Партнер) ТОгда
			Сообщить(""Партнер по коду "" + GLN + "" не найден"");
			Возврат Неопределено;
		КонецЕсли;
	КонецЕсли;
	Возврат Партнер;
	
