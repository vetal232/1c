			Грузополучатель = ПолучитьПартнера(GLNГрузополучателя,,""СправочникСсылка.Контрагенты"");
		КонецЕсли;
		Если ЗначениеЗаполнено(Партнер) и ЗначениеЗаполнено(Контрагент) Тогда			
			Если ПроверитьКонтрагентаДляПартнера(Партнер,Контрагент)=Ложь Тогда				
				Партнер = НайтиПартнераПоКонтрагенту(Контрагент);
			КонецЕсли;
		ИначеЕсли ЗначениеЗаполнено(Контрагент) Тогда
			Партнер = НайтиПартнераПоКонтрагенту(Контрагент);
		КонецЕсли;
		СтруктураВозврата.Вставить(""Контрагент"",Контрагент);
		СтруктураВозврата.Вставить(""Грузополучатель"",Грузополучатель);
		СтруктураВозврата.Вставить(""Партнер"",Партнер);
		ДоговорИСоглашение = ПолучитьДоговорИСоглашение(Контрагент, Организация);
	Иначе
		Контрагент = ПолучитьПартнера(GLNКонтрагента,,""СправочникСсылка.Контрагенты"");
		Грузополучатель = ПолучитьПартнера(GLNГрузополучателя,,""СправочникСсылка.Контрагенты"");
		СтруктураВозврата.Вставить(""Контрагент"",Контрагент);
		СтруктураВозврата.Вставить(""Грузополучатель"",Грузополучатель);
		ДоговорИСоглашение = ПолучитьДоговорИСоглашение(Контрагент, Организация);
	КонецЕсли;
	
	//Если ЗначениеЗаполнено(Партнер) Тогда
		//СтруктураВозврата.Вставить(""Партнер"",Партнер);
		ДоговорИСоглашение = ПолучитьДоговорИСоглашение(Контрагент, Организация, Партнер);
		Попытка
			СтруктураВозврата.Вставить(""ДоговорКонтрагента"", ДоговорИСоглашение.ДоговорКонтрагента);
			СтруктураВозврата.Вставить(""Соглашение"",ДоговорИСоглашение.Соглашение);
		Исключение
			СтруктураВозврата.Вставить(""ДоговорКонтрагента"", """");
			СтруктураВозврата.Вставить(""Соглашение"","""");
		КонецПопытки;
		Попытка
			СтруктураКонтактнойИнформацииПартнер = ПолучитьАдресИЗонуДоставки(Партнер);
			СтруктураВозврата.Вставить(""АдресДоставки"",СтруктураКонтактнойИнформацииПартнер.АдресДоставки);
			СтруктураВозврата.Вставить(""ЗонаДоставки"",СтруктураКонтактнойИнформацииПартнер.ЗонаДоставки);
		Исключение
			ТекстОшибки = ОписаниеОшибки();
			КонтактнаяИнформация = """";
			СтруктураВозврата.Вставить(""АдресДоставки"","""");
			СтруктураВозврата.Вставить(""ЗонаДоставки"","""");
		КонецПопытки;
		
	//Иначе
	//	СтруктураВозврата.Вставить(""Партнер"", """");
	//	СтруктураВозврата.Вставить(""ДоговорКонтрагента"", """");
	//	СтруктураВозврата.Вставить(""Соглашение"","""");
	//	СтруктураВозврата.Вставить(""АдресДоставки"","""");
	//	СтруктураВозврата.Вставить(""ЗонаДоставки"","""");

	//КонецЕсли;		
	
	Возврат СтруктураВозврата;
КонецФункции

&НаСервереБезКонтекста
Функция ПроверитьКонтрагентаДляПартнера(Партнер,Контрагент)
	Запрос = Новый Запрос;
	Запрос.Текст = 
		""ВЫБРАТЬ РАЗРЕШЕННЫЕ
		|	ИерархияПартнеров.Родитель КАК Партнер,
		|	Контрагенты.Ссылка КАК Контрагент,
		|	ИерархияПартнеров.Уровень КАК Уровень
		|ИЗ
		|	РегистрСведений.ИерархияПартнеров КАК ИерархияПартнеров
		|		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.Контрагенты КАК Контрагенты
		|		ПО (ИерархияПартнеров.Родитель = Контрагенты.Партнер)
		|ГДЕ
		|	ИерархияПартнеров.Партнер = &Партнер
		|	И НЕ Контрагенты.ПометкаУдаления"";
		
	
	Запрос.УстановитьПараметр(""Партнер"", Партнер);
	Выборка = Запрос.Выполнить().Выгрузить();
	Отбор = Новый Структура;
    Отбор.Вставить(""Контрагент"", Контрагент);
    Строки = Выборка.НайтиСтроки(Отбор);
	Если Строки.Количество()>0 Тогда
		Возврат Истина;
	КонецЕсли;
	
	Возврат Ложь;
КонецФункции

&НаСервереБезКонтекста
Функция НайтиПартнераПоКонтрагенту(Контрагент)
	Запрос = Новый Запрос;
	Запрос.Текст = 
		""ВЫБРАТЬ
		|	Контрагенты.Партнер
		|ПОМЕСТИТЬ ВТ
		|ИЗ
		|	Справочник.Контрагенты КАК Контрагенты
		|ГДЕ
		|	Контрагенты.Ссылка = &Ссылка
		|;
		|
		|////////////////////////////////////////////////////////////////////////////////
		|ВЫБРАТЬ
		|	Партнеры.Ссылка
		|ИЗ
		|	ВТ КАК ВТ
		|		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Партнеры КАК Партнеры
		|		ПО ВТ.Партнер = Партнеры.Ссылка"";
	
	Запрос.УстановитьПараметр(""Ссылка"", Контрагент);
	
	РезультатЗапроса = Запрос.Выполнить();
	
	ВыборкаДетальныеЗаписи = РезультатЗапроса.Выбрать();
	
	Пока ВыборкаДетальныеЗаписи.Следующий() Цикл
		Возврат ВыборкаДетальныеЗаписи.Ссылка;
	КонецЦикла;
	Возврат Неопределено;
КонецФункции


Функция ПолучитьАдресИЗонуДоставки(Партнер)
	СтрАдресДоставки = """";
	ПартнерОбъект = Партнер.ПОлучитьОбъект();
	Попытка
		СтрокиАдресДоставки = ПартнерОбъект.КонтактнаяИнформация.НайтиСтроки(ПолучитьСТруктуруПоискаКонтактнойИнформации());			
		СтрАдресДоставки = СтрокиАдресДоставки[0].Представление;
	Исключение
		Попытка
			СтрокиАдресДоставки = ПартнерОбъект.КонтактнаяИнформация.НайтиСтроки(ПолучитьСТруктуруПоискаКонтактнойИнформации(1));			
			СтрАдресДоставки = СтрокиАдресДоставки[0].Представление;
		Исключение
			СтрАдресДоставки = """";				
		КонецПопытки;		
	КонецПопытки;
	Попытка
		ЗонаДоставки = Партнер.ЗонаДоставки;
	Исключение
		ЗонаДоставки = Справочники.ЗоныДоставки.ПустаяСсылка();
	КонецПопытки;
	СтруктураВозврата = Новый Структура;
	СтруктураВозврата.Вставить(""АдресДоставки"", СтрАдресДоставки);
	СтруктураВозврата.Вставить(""ЗонаДоставки"", ЗонаДоставки);
	Возврат СтруктураВозврата;
	//Возврат СтруктураВОзврата;
КонецФункции

