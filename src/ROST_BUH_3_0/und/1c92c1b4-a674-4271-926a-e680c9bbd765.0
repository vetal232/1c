Функция ПолучитьНоменклатуруСервер(Поле, Значение,МестоПОискаНоменклатуры)
	СтруктураВозврата = Новый Структура;
	СтруктураВозврата.Вставить(""Номенклатура"");
	СтруктураВозврата.Вставить(""Характеристика"");
	МаркерСообщения = 0;
	Номенклатура = """";
	Если МестоПОискаНоменклатуры = ""По реквизиту"" Тогда
		МаркерСообщения = 1;
		Если (Поле = ""Код"") Тогда
			Номенклатура = Справочники.Номенклатура.НайтиПоКоду(СокрЛП(Значение));
		Иначе
			Номенклатура = Справочники.Номенклатура.НайтиПоРеквизиту(Поле,СокрЛП(Значение));
		КонецЕсли;
		СтруктураВозврата.Номенклатура = Номенклатура;
	ИначеЕсли МестоПОискаНоменклатуры = ""По РС Дополнительные сведения"" Тогда
		МаркерСообщения = 2;
		Если ЗначениеЗаполнено(Поле) Тогда
		Запрос = Новый Запрос;
		Запрос.Текст = ""ВЫБРАТЬ
		               |	ДополнительныеСведения.Объект
		               |ИЗ
		               |	РегистрСведений.ДополнительныеСведения КАК ДополнительныеСведения
		               |ГДЕ
		               |	ДополнительныеСведения.Значение = &Значен
		               |	И ДополнительныеСведения.Свойство.Заголовок = &Наименование"";
		Запрос.УстановитьПараметр(""Наименование"", Поле);
		Запрос.УстановитьПараметр(""Значен"",  Значение);
		Результат = Запрос.Выполнить().Выбрать();
		Если Результат.Следующий() Тогда
			Номенклатура = Результат.Объект.Ссылка;
			СтруктураВозврата.Номенклатура = Номенклатура;
		КонецЕсли;
		Иначе
			Сообщить(""Не указано наименование свойства для поиска номенклатуры"");
		КонецЕсли;
	ИначеЕсли МестоПОискаНоменклатуры = ""По РС Штрихкоды"" Тогда
		МаркерСообщения = 3;
		Запрос = Новый Запрос;
		Запрос.Текст = ""ВЫБРАТЬ
			              |	ШтрихкодыНоменклатуры.Номенклатура.Ссылка
			              |ИЗ
			              |	РегистрСведений.ШтрихкодыНоменклатуры КАК ШтрихкодыНоменклатуры
			              |ГДЕ
			              |	ШтрихкодыНоменклатуры.Штрихкод = &Значение"";
		Запрос.УстановитьПараметр(""Значение"", Значение);
		РЕзультат = Запрос.Выполнить().Выбрать();
		Если Результат.Следующий() Тогда
			Номенклатура = Результат.НоменклатураСсылка;	
			СтруктураВозврата.Номенклатура = Номенклатура;
		КонецЕсли;
	ИначеЕсли МестоПОискаНоменклатуры = ""По справонику Дополнительные реквизиты"" Тогда
		
		Запрос = Новый Запрос;
		Запрос.Текст = ""ВЫБРАТЬ
		               |	Номенклатура.Ссылка
		               |ИЗ
		               |	Справочник.Номенклатура КАК Номенклатура
		               |ГДЕ
		               |	Номенклатура.ДополнительныеРеквизиты.Значение = &Значение
		               |	И Номенклатура.ДополнительныеРеквизиты.Свойство = &СвойствоПВХ"";
		СвойствоПВХ = ПланыВидовХарактеристик.ДополнительныеРеквизитыИСведения.НайтиПоНаименованию(Поле);
		Запрос.УстановитьПараметр(""СвойствоПВХ"", СвойствоПВХ);
		Запрос.УстановитьПараметр(""Значение"",  Значение);
		Результат = Запрос.Выполнить().Выбрать();
		
		Если Результат.Следующий() Тогда
			
			Номенклатура = Результат.Ссылка;
			СтруктураВозврата.Номенклатура = Номенклатура;
			
		КонецЕсли;
	
	Иначе 
		МаркерСообщения = 4;
		Запрос = Новый Запрос;
		Запрос.Текст = ""ВЫБРАТЬ
		               |	НоменклатураПоставщиков.Номенклатура.Ссылка,
		               |	НоменклатураПоставщиков.Характеристика.Ссылка
		               |ИЗ
		               |	Справочник.НоменклатураПоставщиков КАК НоменклатураПоставщиков
		               |ГДЕ
		               |	НоменклатураПоставщиков.Артикул = &артикул"";
		Запрос.УстановитьПараметр(""артикул"", СокрЛП(Значение));
		Результат = Запрос.Выполнить().Выбрать();
		
		Если Результат.Следующий() Тогда 
			
			СтруктураВозврата.Номенклатура =  Результат.НоменклатураСсылка;
			СтруктураВОзврата.Характеристика = Результат.ХарактеристикаСсылка;
			Возврат СтруктураВозврата;
			
		КонецЕсли;
		
	КонецЕсли;
	
	Возврат СтруктураВОзврата;
	
