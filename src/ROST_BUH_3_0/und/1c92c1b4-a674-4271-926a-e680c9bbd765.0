Функция ПолучитьНоменклатуруИзБД(Поле, Код, КОнтрагент = Неопределено)
	
	
	Отбор = ЭДИ_РаботаСБазойДанных.ПолучитьПустуюЗапись();
	Отбор.Тип = ""GOODSCODE"";
	//Отбор.ИндексированныйАтрибут1 = GLN;
	
	
	Если Поле = ""КодПокупателя"" Тогда
		Отбор.ИндексированныйАтрибут1 = Код;
	ИначеЕсли Поле = ""КодПоставщика"" Тогда
		Отбор.ИндексированныйАтрибут2 = Код;
	ИначеЕсли Поле = ""Штрихкод"" Тогда
		Отбор.ИндексированныйАтрибут3 = Код;
	КонецЕсли;
	
	Запрос = ЭДИ_РаботаСБазойДанных.ПолучитьСвойство(Отбор);	
	Если Запрос.Результат=0 тогда
		Сообщить(""Номенклатура по полю "" + Поле + "" и коду ""+Код+"" не найдена! Проверьте правильность заполнения кодов номенклатуры с помощью менеджера свойств номенклатуры."");
		Возврат Неопределено;
	Иначе
		Свойство = Запрос.Содержимое;		
		СтрНоменклатура = Свойство.Объект;
		Возврат ЗначениеИзСтрокиВнутрСервер(СтрНоменклатура);
	КонецЕсли;

