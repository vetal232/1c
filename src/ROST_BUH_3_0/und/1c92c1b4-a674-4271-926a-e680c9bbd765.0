Функция ЗагрузитьFNS_ARCHIVE(Структура)
		
	ДокументЗагруженБезОшибок = Истина;
	
	Документ = Структура.Документ;
	
	Если Не ЗначениеЗаполнено(Документ.Ссылка) Тогда
		НомерНакладной = Структура.НомерНакладной;
		НомерЗаказа = Структура.НомерЗаказа;
		СуммаНДС = Структура.СуммаНДС;
		СуммаСФСНДС = Структура.СуммаСФСНДС;
		СуммаСФБезНДС = Структура.СуммаСФБезНДС;
		Если ЗначениеЗаполнено(НомерНакладной) Тогда
			Поступление = НайтиПоступление(НомерНакладной,Структура.ДатаДокумента, Структура.Контрагент);
		ИначеЕсли ЗначениеЗаполнено(НомерЗаказа) Тогда
			Заказ = НайтиЗаказПоставщику(НомерЗаказа,Структура.ДатаДокумента, Структура.Контрагент);
			Если Не ЗначениеЗаполнено(Заказ) Тогда
				Сообщить(""Не найден заказ с номером: ""+НомерЗаказа+"" дальнейшая загрузка СчФ № ""+Структура.НомерДокумента+"" невозможна."");
				Возврат Неопределено;
			КонецЕсли;
			
			Запрос = Новый Запрос;
			Запрос.Текст = ""ВЫБРАТЬ
			|	ПоступлениеТоваровУслуг.Ссылка
			|ИЗ
			|	Документ.ПоступлениеТоваровУслуг КАК ПоступлениеТоваровУслуг
			|ГДЕ
			|	ПоступлениеТоваровУслуг.Сделка = &Сделка
			|	И ПоступлениеТоваровУслуг.ПометкаУдаления = ЛОЖЬ"";
			Запрос.УстановитьПараметр(""Сделка"",Заказ);
			Результат = Запрос.Выполнить().Выбрать();
			Если Результат.Следующий() Тогда
				Поступление = Результат.Ссылка;
			Иначе
				Поступление = Неопределено;
			КонецЕсли;
			
		КонецЕсли; 
		
		Если ЗначениеЗаполнено(Поступление) Тогда
			СуммаДокумента = ?(Поступление.СуммаВключаетНДС = Истина,СуммаСФСНДС,СуммаСФБезНДС);
			
			Документ = Документы.СчетФактураПолученный.СоздатьДокумент();
			Документ.Заполнить(Поступление.Ссылка);
			Документ.Дата = Структура.ДатаДокумента;
			
			Документ.Комментарий = Документ.Комментарий+""//Сумма в ЭД: "" +СуммаДокумента+ ""; Сумма НДС в ЭД: ""+СуммаНДС+"";// "";
			Документ.НомерВходящегоДокумента =Структура.НомерДокумента;
			Документ.ДатаВходящегоДокумента = Структура.ДатаДокумента;
			Документ.КодСпособаПолучения = 2;
		Иначе
			Сообщить(""При загрузке документа с номером ""+Структура.НомерДокумента+"" от ""+Структура.ДатаДокумента+"" были обнаружены ошибки
			|Не найдено поступление товаров с номером реализации товаров поставщика ""+НомерНакладной+"" или номером заказа ""+НомерЗаказа,СтатусСообщения.Важное);
			Возврат Неопределено;
		КонецЕсли;
	КонецЕсли;
	
	Если Объект.РежимОтладки = Истина Тогда
		Документ.Комментарий = ""ВНИМАНИЕ!!! ЭТО ТЕСТОВЫЙ ДОКУМЕНТ!!! СОХРАНЕНИЕ И ПОСЛЕДУЩЕЕ ИСПОЛЬЗОВАНИЕ В ЭЛЕКТРОННОМ ДОКУМЕНТООБОРОТЕ НЕДОПУСТИМО!!!"";
		Документ.ПолучитьФорму(Документ.Метаданные().ОсновнаяФормаОбъекта.Имя).Открыть();
		Возврат Неопределено;
	КонецЕсли;
	
	Если ДокументЗагруженБезОшибок Тогда
		Попытка
			Документ.Записать();
			//ЗаписатьСвойствоОбъекта(Документ,""EDISoft_Выгружен"",1);
			//ЗаписатьСвойствоОбъекта(Документ,""EDISoft_НомерВходящего"",СтрокаТ.document_number);
			//ЗаписатьСтатусДокумента(Документ.Ссылка,ИмяПостФайла,""1"",Base64Строка(новый ДвоичныеДанные(СтрокаТ.file_name)),ТекущаяДата(),Валидность);			
			////УдалитьФайлы(ФайлПодписи.ПолноеИмя);
			////УдалитьФайлы(ФайлФНС.ПолноеИмя);
			////Попытка УдалитьФайлы(ФайлXML.ПолноеИмя); Исключение КонецПопытки;
			//СтрокаТ.Документ = Документ.Ссылка;
			//СтрокаТ.ДанныеСтатуса = 3;
			//СтрокаТ.СтатусФНС = ""Загружен"";
			
			//Если РасхождениеПоСумме = Истина Тогда
			//	Сообщить(""Есть расхождение между суммой СФ и суммой ФНС документа"");
			//КонецЕсли; 
			СтруктураВозврата = Новый Структура();
			СтруктураВозврата.Вставить(""Документ"",Документ.Ссылка);			
			Возврат СтруктураВозврата;
		Исключение
			Сообщить(ОписаниеОшибки());
		КонецПопытки;
	Иначе
		Сообщить(""При загрузке документа с номером ""+Структура.НомерДокумента+"" от ""+Структура.ДатаДокумента+"" были обнаружены ошибки"");
	КонецЕсли; 
КонецФункции
