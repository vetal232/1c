Функция СформироватьСТруктураОтветаНаЗаказ(Ид)
	СтрокаТ = ЗаголовкиДокументов.НайтиПоИдентификатору(Ид);
	
	СтруктураОтветаНаЗаказ = Новый Структура;
			
	Отбор = ЭДИ_РаботаСБазойДанных.ПолучитьПустуюЗапись();
	Отбор.Тип = ""DOC_ORDRSP"";
	Отбор.ИндексированныйАтрибут1 = ПреобразоватьДатуВСтроку(СтрокаТ.document_date);
	Отбор.ИндексированныйАтрибут2 = СокрЛП(СтрокаТ.document_number);
	Отбор.Владелец = ЗначениеВСтрокуВнутрСервер(СтрокаТ.Контрагент);
	Запрос = ЭДИ_РаботаСБазойДанных.ПолучитьСвойство(Отбор);	
	Если Запрос.Результат=0 тогда
		Сообщить(Запрос.Сообщение);
		Возврат Неопределено;
	Иначе
		Свойство = Запрос.Содержимое;		
		Попытка 
			Документ = СТрокаТ.Документ;
		Исключение
			Документ = Неопределено;
		КонецПопытки;
		Корень = ЗначениеИзСтрокиВнутрСервер(Свойство.ДопАтрибуты);
	КонецЕсли;
	
	//Корень = ПолучитьСТруктуруФайла(СТрокаТ.СтруктураФайла);
	ДокументЗагруженБезОшибок = Истина;
	//Объект.ТаблицаСравненияФайлаИДокумента.Очистить();
	
	Если Не ЗначениеЗаполнено(Документ.Ссылка) Тогда
		Сообщить(""При загрузке документа с номером ""+СтрокаТ.document_number+"" от ""+СтрокаТ.document_date+"" (""+СтрокаТ.file_name+"") были обнаружены ошибки: не найден исходный заказ"");
		Возврат Неопределено;
	Иначе
		СтруктураОтветаНаЗаказ.Вставить(""Документ"", ЗначениеВСтрокуВнутрСервер(Документ.Ссылка));
	КонецЕсли;
	
	СтруктураТЧ = Новый Структура;	
	СтруктураСТроки = Новый СТруктура;	
	СтруктураСТроки.Вставить(""ЦенаБезНДС"");
	СтруктураСТроки.Вставить(""ЦенаСНДС"");
	СтруктураСТроки.Вставить(""Номенклатура"");
	СтруктураСТроки.Вставить(""Количество"");
	КоличествоИтераций = Число(Корень.OrderResponse_Summary.TotalLines);
	Для Счетчик=1 по КоличествоИтераций Цикл
		Попытка
			Тег = ?(КоличествоИтераций = 1,Корень.OrderResponse_Lines.Line,Корень.OrderResponse_Lines.Line[Счетчик-1]);
			
			Попытка Штрихкод = Тег.Line_Item.EAN; Исключение Штрихкод = """"; КонецПопытки; 
			Попытка КодБаер = Тег.Line_Item.BuyerItemCode; Исключение КодБаер = """"; КонецПопытки;
			Попытка КодСапл = Тег.Line_Item.SupplierItemCode; Исключение КодСапл = """"; КонецПопытки;
			Попытка Количество = Число(Тег.Line_Item.AllocatedDelivered); Исключение Количество = 0; КонецПопытки;
			
			Попытка
				ЦенаБезНДС = Число(Тег.Line_Item.OrderedUnitNetPrice);
			Исключение
				ЦенаБезНДС = Число(Тег.Line_Item.OrderedUnitGrossPrice)/(1+Число(Тег.Line_Item.TaxRate)/100);
			КонецПопытки; 			
			Попытка
				ЦенаСНДС = Число(Тег.Line_Item.OrderedUnitGrossPrice);
			Исключение
				ЦенаСНДС = Число(Тег.Line_Item.OrderedUnitNetPrice)*Число(Тег.Line_Item.TaxRate)/100+Число(Тег.Line_Item.OrderedUnitNetPrice);
			КонецПопытки; 
		Исключение
			Сообщить(""В файле не найдена товарная строка №""+Счетчик);
			Продолжить;
		КонецПопытки;
		
		Номенклатура = ПолучитьТовар(КодБаер,КодСапл,Штрихкод);		
		Если НЕ ЗначениеЗаполнено(Номенклатура) Тогда
			ДокументЗагруженБезОшибок = Ложь;
			СтрокаСравнения = Объект.ТаблицаСравненияФайлаИДокумента.Добавить();
			Сообщить( ""Не найденная номенклатура: ""+?(ЗначениеЗаполнено(Штрихкод),"" с штрихкодом ""+Штрихкод+"";"","""")
			+?(ЗначениеЗаполнено(КодБаер),"" с кодом покупателя ""+КодБаер+"";"","""")
			+?(ЗначениеЗаполнено(КодСапл),"" с кодом поставщика ""+КодСапл+"";"","""")
			+?(ЗначениеЗаполнено(СтрокаТ.Контрагент),"" для контрагента ""+СтрокаТ.Контрагент+"";"",""""));
			
			Возврат Неопределено;
			//СтрокаСравнения.Флажок = Истина;
		Иначе
			СТруктураСтроки.ЦенаБезНДС = ЦенаБезНДС;
			СтруктураСтроки.ЦенаСНДС = ЦенаСНДС;
			СтруктураСтроки.Номенклатура = Номенклатура;
			СТруктураСтроки.Количество = Количество;
		КонецЕсли;
		Ключ = ""Строка"" + Строка(Счетчик);
		СтруктураТЧ.Вставить(Ключ, СтруктураСТроки);
	КОнецЦикла;
	СтруктураОтветаНаЗаказ.Вставить(""ТабличнаяЧасть"", СтруктураТЧ);	
	
	ОбъектЗаписи = ЗагрузитьORDRSP(СтруктураОтветаНаЗаказ);
	//
	Если НЕ ЗначениеЗаполнено(ОбъектЗаписи) Тогда					
		
		ЗаписатьНастройкиДляСравнения();
		ФормаСравнения = ПолучитьФорму(""ВнешняяОбработка.EDISOFT.Форма.ФормаСравненияФайлаИДокумента"");	
		ФормаСравнения.ОткрытьМодально();		
					
	Иначе
		
		Сообщить(""Расхождений в документе ""+Документ.Ссылка+"" и ответе на заказ не обнаружено."", СтатусСообщения.Информация);		
		Свойство.Объект = ОбъектЗаписи;
		Свойство.Состояние = 1;
		ЭДИ_РаботаСБазойДанных.ПерезаписатьПоИндексу(Свойство);		
		ВОзврат Истина
		
	КонецЕсли; 	

КонецФункции
&НаКлиентеНаСервереБезКонтекста
