	//	Документ = Документы.СчетФактураВыданный.ПустаяСсылка();
	//КонецЕсли;
	
	Если Документ.Исправление = Истина Тогда
		InvoiceVersionNumber = Формат(Число(Документ.НомерИсправления),""ЧДЦ=0; ЧН=0; ЧРД=.; ЧГ=0"");
